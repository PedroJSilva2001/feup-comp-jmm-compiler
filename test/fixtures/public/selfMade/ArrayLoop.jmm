import io;

class ArrayLoop {

    public int init(int[] arr) {
        int i;
        i = 0;
        while (i < arr.length) {
            arr[i] = i+1;
            i = i+1;
        }

        return 0;
    }

    public int sum(int[] arr) {
        int sum;
        int i;
        sum = 0;

        i = 0;
        while (i < arr.length) {
            sum = sum + arr[i];
            i = i + 1;
        }

        return sum;
    }

    public static void main(String[] args) {
        ArrayLoop arrayLoop;
        int[] arr;

        arrayLoop = new ArrayLoop();
        arr = new int[100];
        arrayLoop.init(arr);

        io.println(arrayLoop.sum(arr));
    }

}

import io;

class NestedWhilesIfs {

    public int call(int arg1, int arg2, int arg3, int arg4) {
        int i1;
        int i2;
        int i3;
        int i4;
        int res;

        res = 0;

        i1 = 0;
        while (i1 < arg1) {
            i2 = 0;
            while (i2 < arg2) {

                if (i1 < i1) {
                    i2 = i2 + 1000;
                } else {
                    if (i2 < i2) {
                        i2 = i2 + 1000;
                    } else {
                        i2 = i2;
                    }
                }

                i3 = 0;
                while (i3 < arg3) {

                    while (i3 < i3) {
                        i3 = i3 + 10000;
                    }

                    i4 = 0;
                    while (i4 < arg4) {
                        res = res + 1;
                        i4 = i4 + 1;
                    }
                    i3 = i3 + 1;
                }
                i2 = i2 + 1;
            }
            i1 = i1 + 1;
        }

        return res;
    }

    public static void main(String[] args) {
        NestedWhilesIfs obj;
        obj = new NestedWhilesIfs();

        io.println(obj.call(1, 2, 3, 4)); // 24
    }

}

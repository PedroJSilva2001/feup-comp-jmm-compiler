import io;

class SelfMade1Fields {
    int i;
    int div;

    public int call(int num, boolean bo){
        i = 0;
        while (bo && 0 < num) {
            div = num / 2;
            div = 2 * div;

            if (!(div < num) && !(num < div))
                i = i + 1;
            else
                i = i;

            num = num - 1;
        }

        return i;
    }

    public static void main(String[] args){
        io.println(new SelfMade1Fields().call(10, true));
    }
}
